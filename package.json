{
  "devDependencies": {
    "babel-plugin-styled-components": "^2.1.4"
  },
  "babel": {
    "env": {
      "development": {
        "presets": ["next/babel"],
        "plugins": [
          [
            "styled-components",
            { "ssr": true, "displayName": true, "preprocess": false }
          ]
        ]
      },
      "production": {
        "presets": ["next/babel"],
        "plugins": [
          [
            "styled-components",
            { "ssr": true, "displayName": false, "preprocess": false }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "styled-components": "^5.3.10"
  }
}
